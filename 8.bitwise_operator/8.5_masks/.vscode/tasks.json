{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Buid with GCC",
			"command": "/usr/bin/g++-12",
			"args": [
				"-g",
				"-std=c++20",
				"${workspaceFolder}/*.cpp",
				"-o",
				"${fileDirname}/rooster"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": ["$gcc"],
			"group": "build",
			"detail": "compiler: /usr/bin/g++-12"
		},
